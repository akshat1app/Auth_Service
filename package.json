    "@nestjs/passport": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@types/mocha": "^10.0.10",
    "dotenv": "^16.5.0",
    "google-auth-library": "^9.15.1",
    "ioredis": "^5.6.1",
